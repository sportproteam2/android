<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/news_nav_graph"
    app:startDestination="@id/newsFragment">

    <fragment
        android:id="@+id/newsFragment"
        android:name="com.example.test_sportpro.ui.fragments.NewsFragment"
        tools:layout="@layout/fragment_news">
        <action
            android:id="@+id/action_NewsFragment_to_articleFragment"
            app:destination="@id/articleFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_NewsFragment_to_filterFragment"
            app:destination="@id/filterFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_NewsFragment_to_judgeFragment"
            app:destination="@id/judgeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.test_sportpro.ui.fragments.ProfileFragment"
        android:label="Профиль"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_numberFragment"
            app:destination="@id/numberFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profileFragment_to_mainProfileFragment"
            app:destination="@id/mainProfileFragment" />
        <action
            android:id="@+id/action_profileFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>


    <fragment
        android:id="@+id/articleFragment"
        android:name="com.example.test_sportpro.ui.fragments.ArticleFragment"
        tools:layout="@layout/fragment_article">
        <argument
            android:name="article"
            app:argType="com.example.test_sportpro.models.ArticleItem" />
    </fragment>


    <fragment
        android:id="@+id/numberFragment"
        android:name="com.example.test_sportpro.NumberFragment"
        android:label="fragment_number"
        tools:layout="@layout/fragment_number">
        <action
            android:id="@+id/action_numberFragment_to_codeFragment"
            app:destination="@id/codeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="status"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>


    <fragment
        android:id="@+id/codeFragment"
        android:name="com.example.test_sportpro.CodeFragment"
        android:label="fragment_code"
        tools:layout="@layout/fragment_code">
        <action
            android:id="@+id/action_codeFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="code"
            android:defaultValue="0"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="numberForTextView"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="0" />
        <argument
            android:name="statuss"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />


    </fragment>
    <fragment
        android:id="@+id/confirmationFragment"
        android:name="com.example.test_sportpro.ConfirmationFragment"
        android:label="fragment_confirmation"
        tools:layout="@layout/fragment_confirmation" />

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.test_sportpro.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_registerFragment_to_confirmationFragment"
            app:destination="@id/confirmationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_registerFragment_to_mainProfileFragment"
            app:destination="@id/mainProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/filterFragment"
        android:name="com.example.test_sportpro.ui.fragments.FilterFragment"
        android:label="Фильтровать"
        tools:layout="@layout/fragment_filter">
        <action
            android:id="@+id/action_filterFragment_to_sportTypesFragment"
            app:destination="@id/sportTypesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/sportTypesFragment"
        android:name="com.example.test_sportpro.ui.fragments.SportTypesFragment"
        tools:layout="@layout/fragment_sport_types">
        <argument
            android:name="title"
            app:argType="string" />
        <action
            android:id="@+id/action_sportTypesFragment_to_newsFragment"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.test_sportpro.ui.fragments.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" />

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.test_sportpro.ui.fragments.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" />

    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.test_sportpro.ui.fragments.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third">
        <action
            android:id="@+id/action_thirdFragment_to_thirdInfoFragment"
            app:destination="@id/thirdInfoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/mainProfileFragment"
        android:name="com.example.test_sportpro.ui.fragments.MainProfileFragment"
        android:label="fragment_main_fragment"
        tools:layout="@layout/fragment_main_fragment" >
        <action
            android:id="@+id/action_mainProfileFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
        <action
            android:id="@+id/action_mainProfileFragment_to_competitionDetailFragment"
            app:destination="@id/competitionDetailFragment" />
        <action
            android:id="@+id/action_mainProfileFragment_to_programCompetitonFragment"
            app:destination="@id/programCompetitonFragment" />
    </fragment>

    <fragment
        android:id="@+id/comAllFragment"
        android:name="com.example.test_sportpro.ComAllFragment"
        android:label="fragment_com_all"
        tools:layout="@layout/fragment_com_all"/>

    <fragment
        android:id="@+id/comMyFragment"
        android:name="com.example.test_sportpro.ComMyFragment"
        android:label="fragment_com_my"
        tools:layout="@layout/fragment_com_my"/>

    <fragment
        android:id="@+id/competitionDetailFragment"
        android:name="com.example.test_sportpro.CompetitionDetailFragment"
        android:label="fragment_competition_detail"
        tools:layout="@layout/fragment_competition_detail" />

    <!--        tools:layout="@layout/fragment_com_my" />-->
    <fragment
        android:id="@+id/thirdInfoFragment"
        android:name="com.example.test_sportpro.ui.fragments.ThirdInfoFragment"
        android:label="Назад" />

    <fragment
        android:id="@+id/judgeFragment"
        android:name="com.example.test_sportpro.ui.fragments.JudgeFragment">
        <action
            android:id="@+id/action_judgeFragment_to_chooseCompetitionFragment"
            app:destination="@id/chooseCompetitionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/chooseCompetitionFragment"
        android:name="com.example.test_sportpro.ui.fragments.ChooseCompetitionFragment"
        android:label="Назад">
        <action
            android:id="@+id/action_chooseCompetitionFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.example.test_sportpro.ui.fragments.CategoriesFragment"
        android:label="Назад">
        <action
            android:id="@+id/action_categoriesFragmen_to_judgeCompetitionFragment"
            app:destination="@id/judgeCompetitionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/judgeCompetitionFragment"
        android:name="com.example.test_sportpro.ui.fragments.JudgeCompetitionFragment"
        android:label="Назад">
        <action
            android:id="@+id/action_judgeCompetitionFragment_to_competitionsGridFragment"
            app:destination="@id/competitionsGridFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/competitionsGridFragment"
        android:name="com.example.test_sportpro.ui.fragments.CompetitionsGridFragment"
        android:label="Назад">
        <action
            android:id="@+id/action_competitionsGridFragment_to_gridsDetailGridFragment"
            app:destination="@id/gridsDetailFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/gridsDetailFragment"
        android:name="com.example.test_sportpro.ui.fragments.GridsDetailFragment"
        android:label="Назад" />
    <fragment
        android:id="@+id/programCompetitonFragment"
        android:name="com.example.test_sportpro.ProgramCompetitonFragment"
        android:label="fragment_program_competiton"
        tools:layout="@layout/fragment_program_competiton" />

</navigation>